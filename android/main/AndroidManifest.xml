            <action android:name="android.intent.action.SEVENTH" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".ObjectOptions">
        <intent-filter>
            <action android:name="android.intent.action.SEVENTH" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
        <activity android:name=".RegisterActivity">
