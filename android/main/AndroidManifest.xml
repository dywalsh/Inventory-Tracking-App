        </activity>
        <activity android:name=".useScannerResultAddProject">
            <intent-filter>
                <action android:name="android.intent.action.FIFTH" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name=".ProjectOptions">
        <intent-filter>
            <action android:name="android.intent.action.SIXTH" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".Myprojects" />
    <activity android:name=".ObjsByDate"/>
        <activity android:name=".Search" />
        <activity android:name=".SearchResult" />


        <activity android:name=".AddProject">
